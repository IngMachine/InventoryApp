<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fredy.inventoryapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registro"
            android:parentActivityName=".Usuario"/>
        <activity android:name=".Usuario" />
        <activity android:name=".Lista" />
        <activity android:name=".Herramienta"
            android:parentActivityName=".Lista"/>
        <activity android:name=".Epp"
            android:parentActivityName=".Lista"/>
        <activity android:name=".Dotacion"
            android:parentActivityName=".Lista"/>
    </application>

</manifest>